	function chatChecking(){
		$chatUsers = Array();
		$chatCurr = Array();
		$chatUsers = $.parseJSON($.cookie('chat'));
		//console.log($chatUsers);
		if($('.pmContent').length > 0)$('.pmContent').each(function(index, element) {$chatCurr.push($(this).attr('rel'));});
		if(!!$chatUsers){
			$.each($chatUsers, function(index, value){
				if($chatCurr.indexOf(value) == -1)$.get('ajax/userInfo/getName.php?u='+value,function($data){addChat(value,$data)})
			})
		}
		//alert('checking...');
			
	}
	function addChat($code,$name){
		var $chatUser = Array();
		//alert($code + $name);
		//Nếu chưa có khung chat => tạo khung
		if($('#pmWrap').length == 0)$('body').append('<div id="pmWrap"><div id="pmList"><ul></ul></div><div id="pmContentWrap"></div></div>');
		
		
		//Nếu chưa có cookie => tự tạo mảng
		if($.cookie('chat') == null) $chatUser[0] = $code;
		
		//Nếu đã có cookie => xử lý cookie
		else {
			$chatCurr = Array();
			if($('.pmContent').length > 0)$('.pmContent').each(function(index, element) {$chatCurr.push($(this).attr('rel'));});
			if($chatCurr.indexOf($code) != -1 && $chatCurr.length > 0) return false;
			$chatUsers = $.parseJSON($.cookie('chat'));
			$.each($chatUsers, function (index, value) {$chatUser[index] = value});
			$chatUser.push($code);
		}
		
		var $key = '';
		
		$.each($chatUser, function(key, value){if(value == $code) $key = key;});
		$chatJson = JSON.stringify($chatUser);
		$.cookie('chat', $chatJson,{path: '/'});
		
		$('#pmList .active').removeClass('active');
		$('.pmContent').hide();
		
		var app  = '';
			app += '<li id="cList';
			app += $key;
			app += '" rel="';
			app += $key;
			app += '" class="active">';
			app += $name;
			app += '<a href="javascript:;" rel="';
			app += $key;
			app += '"><span class="fa icon-times"></span></a></li>';
		//alert(app);	
		var cont = '';
			cont += '<div class="pmContent" id="chat';
			cont += $key;
			cont += '" style="display:block" rel="';
			cont += $code;
			cont += '">';
			cont += '<iframe name="chatFrame';
			cont += $key;
			
			cont += '" id="chatFrame';
			cont += $key;
			cont += '" frameborder="0" scrolling="no"';
			cont += 'src="chat/';
			
			cont += $code;
			
			cont += '.html#bottom" width="100%" height="310px"></iframe></div>';
		
		$('#pmList ul').append(app);
		$('#pmContentWrap').append(cont);
		
		chatInit();
	}
	function chatInit(){
		$('#pmList li').click(function(e) {
			var rel = $(this).attr('rel');
			
			$('#pmList .active').removeClass('active');
			$(this).addClass('active');
			
			$('.pmContent').hide();
			$('#chat'+rel).show();
			
			var $f = $("#chatFrame"+rel);
			window.frames['chatFrame'+rel].ScrollDown();
		}).children().click(function(e) {
			$chatUser = Array();
			rel= $(this).attr('rel');

			$('#chat'+rel).remove();
			$('#cList'+rel).remove();
			$chatUsers = $.parseJSON($.cookie('chat'));
			$.each($chatUsers, function (index, value) {$chatUser[index] = value;});
			$chatUser.splice(rel, 1);
			$chatJson = JSON.stringify($chatUser);
			$.cookie('chat', $chatJson,{path: '/'});
			if($('.pmContent').length == 0) {
				$('#pmWrap').remove();
				$.cookie('chat', null, {expires: -1 ,path: '/'});
			}else{
				$('.pmContent:first-child').show();
				$('#pmList li:first-child').addClass('active');
			}
			return false;
		});
	}

	$(document).ready(function(e) {
		chatInit();
		setInterval(function(){chatChecking()},10000);
	})
</script>
 
